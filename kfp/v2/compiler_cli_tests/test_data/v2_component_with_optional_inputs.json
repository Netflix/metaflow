            },
            "inputs": {
              "parameters": {
                "input1": {
                  "runtimeValue": {
                    "constantValue": {
                      "stringValue": "Hello"
                    }
                  }
                },
                "input2": {
                  "runtimeValue": {
                    "constantValue": {
                      "stringValue": "World"
                    }
                  }
                }
              }
            },
            "taskInfo": {
              "name": "component-op"
            }
          }
        }
      }
    },
